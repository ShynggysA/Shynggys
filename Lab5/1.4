def division(list_A):
    list_A.sort()
    n = len(list_A)
    q1 = list_A[:n // 4]
    q2 = list_A[n // 4:(n // 4) * 2]
    q3 = list_A[(n // 4) * 2:(n // 4) * 3]
    q4 = list_A[(n // 4) * 3:]
    
    while len(q1) < n // 4:
        q1.insert(0, 0)
    
    return q1, q2, q3, q4

list_A = [1, 5, 8, 7, 9, 6, 7, 5, 8, 3, 9, 10, 2, 1, 3, 4]
q1, q2, q3, q4 = division(list_A)

print("q1 =", q1)
print("q2 =", q2)
print("q3 =", q3)
print("q4 =", q4)
